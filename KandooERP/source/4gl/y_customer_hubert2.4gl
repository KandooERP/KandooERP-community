				INITIALIZE lup_customer.* TO NULL		                                                                                        	#@G00495
				DISPLAY BY NAME lup_customer.*		                                                                                           	#@G00496
						                                                                                                                         	#@G00496
			WHEN sql_stmt_status < 0		                                                                                              	#@G00497
				CALL display_eric_error("Remove customer:failed ")		                                                                        	#@G00498
				ROLLBACK WORK		                                                                                                            	#@G00499
						                                                                                                                         	#@G00499
			END CASE		                                                                                                              	#@G00500
			EXIT WHILE		                                                                                                            	#@G00501
		END CASE		                                                                                                               	#@G00502
	END WHILE		                                                                                                               	#@G00503
	RETURN sql_stmt_status		                                                                                                  	#@G00504
END FUNCTION		## frm_Remove_customer(pky)		                                                                                	#@G00505


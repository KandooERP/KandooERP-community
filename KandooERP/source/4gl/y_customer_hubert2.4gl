FUNCTION sql_status_pk_customer(pky)  		                                                                                   	#@G00775
##   sql_status_pk_customer : Check if primary key exists		                                                                	#@G00776
## inbound parameter : record of primary key		                                                                             	#@G00777
## outbound parameter:  STATUS > 0  if exists, 0 if no record, < 0 if error		                                              	#@G00778
	DEFINE pky RECORD 		                                                                                                      	#@G00779
		cmpy_code LIKE customer.cmpy_code,   # CHAR(2)		                                                                           	#@G00779
		cust_code LIKE customer.cust_code   # CHAR(8)				                                                                                                                         	#@G00780
	END RECORD		                                                                                                              	#@G00781
	DEFINE pk_status INTEGER		                                                                                                	#@G00782

WHENEVER ERROR CONTINUE		                                                                                                  	#@G00784
	OPEN crs_pky_crs_ USING  pky.*		                                                                                          	#@G00785
	FETCH crs_pky_crs_ 		                                                                                                     	#@G00786
WHENEVER ERROR CALL error_mngmt		                                                                                          	#@G00787

	CASE sqlca.sqlcode		                                                                                                      	#@G00789
	WHEN 0 		                                                                                                                 	#@G00790
		let pk_status = 1		                                                                                                      	#@G00791
	WHEN 100		                                                                                                                	#@G00792
		let pk_status = 0		                                                                                                      	#@G00793
	WHEN sqlca.sqlerrd[2] = 104		                                                                                             	#@G00794
		let pk_status = -1		# record locked		                                                                                    	#@G00795
	WHEN sqlca.sqlcode < 0		                                                                                                  	#@G00796
		let pk_status = sqlca.sqlcode		                                                                                          	#@G00797
	END CASE		                                                                                                                	#@G00798

	RETURN pk_status		                                                                                                        	#@G00800
END FUNCTION		## sql_status_pk_customer		                                                                                  	#@G00801


# Open the QBE CURSOR,		                                                                                                   	#@G00557
# counts returned rows_count,		                                                                                            	#@G00558
# OPEN the data SET,		                                                                                                     	#@G00559
# FETCH first row		                                                                                                        	#@G00560
# inbound parameter: query predicate		                                                                                     	#@G00561
# outbound parameters: number of rows retried		                                                                            	#@G00562
FUNCTION sql_get_qbe_count_customer(qry_stmt)		                                                                            	#@G00563
	DEFINE qry_stmt STRING		                                                                                                  	#@G00564
	DEFINE rec_customer STRING		                                                                                              	#@G00565
	DEFINE rows_count INTEGER		                                                                                               	#@G00566
	DEFINE lsql_stmt_status INTEGER		                                                                                         	#@G00567

	# define primary_key record		                                                                                             	#@G00569
	DEFINE l_pky RECORD 		                                                                                                    	#@G00570
		cmpy_code LIKE customer.cmpy_code,   # CHAR(2)		                                                                           	#@G00570
		cust_code LIKE customer.cust_code   # CHAR(8)				                                                                                                                         	#@G00571
	END RECORD		                                                                                                              	#@G00572

	LET rec_customer = 		                                                                                                     	#@G00574
		"SELECT count(*) FROM customer",		                                                                                       	#@G00575
		" WHERE ",qry_stmt CLIPPED		                                                                                             	#@G00576
		

	PREPARE rec_nbr_crs_ FROM rec_customer		                                                                                  	#@G00579
	DECLARE crs_nbr_crs_ CURSOR for rec_nbr_crs_		                                                                            	#@G00580

	OPEN crs_nbr_crs_		                                                                                                       	#@G00582
	SET ISOLATION TO DIRTY READ		                                                                                             	#@G00583
	WHENEVER ERROR CONTINUE		                                                                                                 	#@G00584
	FETCH crs_nbr_crs_ INTO rows_count		                                                                                      	#@G00585
	WHENEVER ERROR CALL error_mngmt		                                                                                         	#@G00586
	SET ISOLATION TO COMMITTED READ		                                                                                         	#@G00587

	# if FETCH fails, count = 0, the, get back TO query		                                                                     	#@G00589
	IF sqlca.sqlcode OR rows_count = 0 THEN		                                                                                 	#@G00590
		let rows_count =0		                                                                                                      	#@G00591
	END IF		                                                                                                                  	#@G00592
	FREE crs_nbr_crs_		                                                                                                       	#@G00593
	RETURN rows_count		                                                                                                       	#@G00594
END FUNCTION		## sql_get_qbe_count_customer		                                                                              	#@G00595


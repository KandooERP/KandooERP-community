database kandoodb

screen
{
                                                                       M160

 Parent Product          [f000           ] [f001                          ]

 Existing Child Product  [f002           ] [f003                          ]

  Confirm the above deletion (Y/N)?[f]

}

tables
bor

attributes
f    = formonly.answer type char, include = ("Y","N"), upshift;
f000 = bor.parent_part_code;
f001 = formonly.parent_description;
f002 = bor.part_code;
f003 = formonly.old_description;

instructions
delimiters "  "

				<Label classNames="fcls_sblabel" fieldTable="formonly" gridItemLocation="0,1,1,1" identifier="lbInfo2" isDynamic="true" text="" visible="true"/>
			</GridPanel>
		</GridPanel>
	</form.rootContainer>
	<form.screenRecords>
		<ScreenRecord elements="6" fields="formonly.scroll_flag,offersale.offer_code,offersale.desc_text" identifier="sr_offersale"/>
		<ScreenRecord fields="formonly.scroll_flag" identifier="formonly"/>
		<ScreenRecord fields="offersale.offer_code,offersale.desc_text" identifier="offersale"/>
	</form.screenRecords>

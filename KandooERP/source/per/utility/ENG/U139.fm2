					<GridRowDefinition gridLengthValue=""/>
					<GridRowDefinition gridLengthValue=""/>
					<GridRowDefinition gridLengthValue=""/>
				</GridPanel.gridRowDefinitions>
				<GridPanel.gridColumnDefinitions>
					<GridColumnDefinition gridLengthValue="100%"/>
				</GridPanel.gridColumnDefinitions>
				<TextField dataType="Char,,,," classNames="fcls_data_widget,fcls_align_hstretch" preferredSize="24qch,1qch" gridItemLocation="0,0,1,1" fieldTable="formonly" identifier="heading_text"/>
				<Label text="Description" classNames="fcls_fieldlabel,fcls_align_left" gridItemLocation="0,1,1,1" fieldTable="" identifier="label2"/>
				<TextField dataType="Char,,,," classNames="fcls_data_widget,fcls_align_hstretch" preferredSize="21qch,1qch" gridItemLocation="0,2,1,1" fieldTable="formonly" identifier="prompt_text"/>
			</GridPanel>
			<Table rowCount="6" preferredSize="53qch,6qch" gridItemLocation="0,2,1,1" fieldTable="" identifier="sr_userref">
				<TableColumn fieldTable="" identifier="scroll_flag">
					<TableColumn.columnLength>
						<GridLength gridLengthValue="2rem" gridMaxLength="2rem" gridMinLength="2rem"/>
					</TableColumn.columnLength>
					<CheckBox dataType="Char,,,," fieldTable="formonly" identifier="scroll_flag">
						<CheckBox.checkedValue>
							<StringLiteral stringValue="*"/>
						</CheckBox.checkedValue>
						<CheckBox.uncheckedValue>
							<NullLiteral/>
						</CheckBox.uncheckedValue>
					</CheckBox>
				</TableColumn>
				<TableColumn text="Code" fieldTable="" identifier="ref_code">
					<TableColumn.columnLength>
						<GridLength gridLengthValue="10rem" gridMaxLength="10rem" gridMinLength="10rem"/>
					</TableColumn.columnLength>
					<TextField toCase="Up" dataType="Char,,,,10" toolTip=" Reference Code" fieldTable="userref" comment=" Reference Code" identifier="ref_code"/>
				</TableColumn>
				<TableColumn text="Description" fieldTable="" identifier="ref_desc_text">
					<TableColumn.columnLength>
						<GridLength gridLengthValue="100%"/>
					</TableColumn.columnLength>
					<TextField dataType="Char,,,,30" toolTip=" Description relating to the Reference Code" fieldTable="userref" comment=" Description relating to the Reference Code" identifier="ref_desc_text"/>
				</TableColumn>
			</Table>
			<GridPanel classNames="fcls_statusbar_cnt" visible="true" verticalAlignment="Top" gridItemLocation="0,3,1,1" fieldTable="formonly" identifier="cntStatusBar">

	<form.screenRecords>
		<ScreenRecord identifier="sr_userref" fields="scroll_flag,ref_code,ref_desc_text" elements="6"/>
		<ScreenRecord identifier="formonly" fields="scroll_flag,heading_text,prompt_text"/>
		<ScreenRecord identifier="userref" fields="ref_code,ref_desc_text"/>
	</form.screenRecords>
</form>


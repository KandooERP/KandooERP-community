	</form.rootContainer>
	<form.screenRecords>
		<ScreenRecord identifier="sr_product" fields="formonly.scroll_flag,product.part_code,product.desc_text,prodnote.note_date" elements="13"/>
		<ScreenRecord identifier="formonly" fields="formonly.scroll_flag"/>
		<ScreenRecord identifier="prodnote" fields="prodnote.note_date"/>
		<ScreenRecord identifier="product" fields="product.part_code,product.desc_text,product.oem_text"/>
	</form.screenRecords>
</form>


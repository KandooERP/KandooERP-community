	<form.screenRecords>
		<ScreenRecord identifier="sr_maingrp" fields="formonly.scroll_flag,maingrp.maingrp_code,maingrp.desc_text" elements="6"/>
		<ScreenRecord identifier="formonly" fields="scroll_flag"/>
		<ScreenRecord identifier="maingrp" fields="maingrp_code,desc_text"/>
